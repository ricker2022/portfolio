body
{
    background:#ffffff;
    font-family: 微軟雅黑體,微軟正黑體 , MS PMincho, 新細明體;
}
body,div,ul,li,p,img,span,table
{
    padding:0;
    margin:0;
}
ul{list-style:none;}
a{border: 0px;text-decoration: none;}
img{border:0}


.header
{
	width:100%;
	height:83px;
    background:url(../image/headerBg.jpg) repeat-x;
	color:#03C;
}
.header div
{
	width:1000px;
	margin:0 auto;
}
.header ul.menu li
{
	float:left; 
}
.header ul.menu li a
{
	display:block;
	width:123px;
	height:83px;
	line-height:70px;
	text-align:center;
	font-size:18px;
}

.menuBtn
{
	background:url(../image/menuBarBg.jpg) no-repeat right;
	color:#00b0ff;
}
.menuBtn:hover
{
	background:url(../image/menuBtn_over.jpg) no-repeat right;
	color:#ffffff;
}
.menuBtnActive{background:url(../image/menuBarBg.jpg) no-repeat right;}

#waninLogoA a
{
    background:none
}
#waninLogoA a:hover
{
    background:none
}
.section
{
	width:100%;
    background:url(../image/midBg.jpg) repeat-x;
}
.iframeBox li{
	float:left
}
.iframeBg{
	background:url(../image/section03.jpg) no-repeat right top;
}
.menufooter
{
	width:100%;
	height:223px;
    background:url(../image/menuBg.jpg) repeat-x;
	color:#03C;
}
.menufooter div
{
	width:1200px;
	margin:0 auto;
}
.menufooter ul
{
	float:left;
	background:url(../image/footerBar.jpg) no-repeat top left;
	margin:22px 50px 0 50px;
	padding-left:10px;
	height:179px;
}
.menufooter h1
{
	color:#ffffff;
	font-size:18px;
	font-weight:500;
	margin:0;
	padding:0;
	margin-bottom:10px;
}
.menufooter a
{
	color:#ffffff;
	font-size:12px;
	margin-bottom:12px;
	cursor:pointer;
}
.menufooter li a:hover
{
	color:#45d5ff;
	font-size:12px;
}

.footer
{
	width:100%;
	height:50px;
    background:url(../image/bottomBg.jpg) repeat-x;
	text-align:center;
}

#menu {
	position: relative;
	float: left;
	margin:30px 0 0 160px;
    margin-left:160px;
    list-style: none;
    width:770px;
    background: url('../images/newBG/mainBarBg.png') no-repeat;
    padding-left:50px;
    height: 75px;
}
#menu li {
	float: left;
	line-height:75px;padding:0 8px;
}
#menu li a{
	cursor:pointer;
}