    <script src="css/pageTab.js" type="text/javascript"></script>
    <link href="css/subPage.css" rel="stylesheet" type="text/css" />
    <script src="css/cookie.js" type="text/javascript"></script>
    <style type="text/css">
        .provtext {
            font-size: 12px;
        }

        .fontLeft {
            text-align: left;
        }
    </style>
    <script type="text/javascript">
        $(function () {
            if ($('.l1').eq(0).val() == '<%= language %>') {
                $('.l1').eq(0).click();

            } else if ($('.l2').eq(0).val() == '<%= language %>') {
                $('.l2').eq(0).click();
            }
            <% if (provCtrl)
               {%>
            $('.subTagM').children().eq(parseInt('<%= provNo %>') - 1).addClass('active').click();   
               <%}%>

            $('.subTagM').show();
            $('.tab_container').show();
        });


        var l_type = '繁體中文';
        var tabName = ['', '使用者合約', '廠商條款', '隱私權條款', '個人資料使用同意書', '使用者合约', '厂商服务条款', '隐私权条款', '个人资料使用同意书', 'User Agreement', 'Terms of Partners', 'Privacy Policy', '個人資料使用同意書']

        function ltChange(btnNo) {
            tempL_type = $('.l_btn').eq(btnNo).val();
            if (tempL_type == '繁體中文') {
                type = 0;
            } else if (tempL_type == '简体中文') {
                type = 4;
            } else {
                type = 8;
            }
            $('.l_btn').eq(btnNo).blur();
            if (btnNo == 0) {
                $('.l1').each(function () {
                    $(this).val(l_type);
                });
            } else {
                $('.l2').each(function () {
                    $(this).val(l_type);
                });
            }
            if (l_type != 'English') {
                $('.l3').each(function () {
                    $(this).val($('.l_btn').eq(btnNo).val());
                });
            }
            l_type = tempL_type;
            $('#list06').children().each(function () {
                tabNo = parseInt(type) + $(this).index() + 1;
                if (tabNo == 12) {
                    //$(this).children().eq(0).attr('href', '#tab4');
                    $(this).css('display', 'none');
                } else {
                    $(this).show();
                    $(this).children().eq(0).attr('href', '#tab' + tabNo);
                }
                $(this).children().eq(0).text(tabName[tabNo]);
            });
            $('.active').children().eq(0).click();
        }

        function ltChange2() {
            temp = $('.l3').val();
            if (temp == '繁體中文') {
                $('.l3').each(function () {
                    $(this).val('简体中文');
                });
            } else {
                $('.l3').each(function () {
                    $(this).val('繁體中文');
                });
            }
            btn1 = $('.l_btn').eq(0).val();
            btn2 = $('.l_btn').eq(1).val();
            if (btn1 == temp) {
                $('.l_btn').eq(0).click();
            }
            if (btn2 == temp) {
                $('.l_btn').eq(1).click();
            }
        }
    </script>
</asp:Content>
