@using Wb.Models;
@using Wb.Common;
@model 會員
@{
    業餘戰隊 ateam = ViewBag.ateam;
}
<div  class="sub_navi" >
    <div class="col_2"><a href="javascript:window.history.back()" class="back_btn"><i class="fa fa-arrow-left"></i></a></div>
    <div class="col_8 category_title" >會員資料</div>
    <br clear="all"/>
</div>
<div id="member_name">@Model.暱稱</div>
<div id="member_pic">
    @Html.MemberIcon(Model)   
</div>

<form>
<table  id="member_info_table" cellpadding="0" cellspacing="0" >
    <tbody>
        <tr>
            <td >稱號</td>
            <td >@Model.特殊稱號</td>
        </tr>
        <tr>
            <td >生日</td>
            <td >@string.Format("{0:yyyy-MM-d}", Model.生日)</td>
        </tr>
        <tr>
            <td >E-mail</td>
            <td >@Model.電子信箱</td>
        </tr>   
        <tr>
            <td >戰隊</td>
            <td >@ateam.戰隊隊名</td>
        </tr>              
    </tbody>
</table>
                
</form>